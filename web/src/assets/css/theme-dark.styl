  --el-fill-color-light: rgba(86, 86, 95, .2);
--el-color-primary-light-9:rgba(86, 86, 95, .2);
--chat-wel-bg:#2d2f388a;
  --theme-text-color-secondary: #a3aed0;
  // --el-pagination-button-bg-color: rgba(86,86,95,0.2); 

   //layout 
